<p id="notice"><%= notice %></p>

<h1>Documents</h1>

<table>
  <thead>
    <tr>
      <th>Order</th>
      <th>Cliente</th>
      <th>Proveedor</th>
      <th>Sku</th>
      <th>Fechaentrega</th>
      <th>Cantidad</th>
      <th>Cantidaddespachada</th>
      <th>Preciounitario</th>
      <th>Canal</th>
      <th>Estado</th>
      <th>Notas</th>
      <th>Rechazo</th>
      <th>Anulacion</th>
      <th>Urlnotificacion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @documents.each do |document| %>
      <tr>
        <td><%= document.order_id %></td>
        <td><%= document.cliente %></td>
        <td><%= document.proveedor %></td>
        <td><%= document.sku %></td>
        <td><%= document.fechaEntrega %></td>
        <td><%= document.cantidad %></td>
        <td><%= document.cantidadDespachada %></td>
        <td><%= document.precioUnitario %></td>
        <td><%= document.canal %></td>
        <td><%= document.estado %></td>
        <td><%= document.notas %></td>
        <td><%= document.rechazo %></td>
        <td><%= document.anulacion %></td>
        <td><%= document.urlNotificacion %></td>
        <td><%= link_to 'Show', document %></td>
        <td><%= link_to 'Edit', edit_document_path(document) %></td>
        <td><%= link_to 'Destroy', document, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Document', new_document_path %>
