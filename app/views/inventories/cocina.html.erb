<p id="notice"><%= notice %></p>

<h3>COCINA</h3>

<table class="table table-striped">
  <thead >
    <tr>
      <th scope="col">SKU</th>
      <th scope="col">NOMBRE</th>
      <th scope="col">CANTIDAD</th>
    </tr>
  </thead>

  <tbody>
    <% @cocina.each do |product| %>
      <tr>
        <td scope="row"><%= product['sku'] %></td>
        <td><%= product['nombre'] %></td>
        <td><%= product['cantidad'] %></td>
      </tr>
    <% end %>
  </tbody>
</table>